<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping package="com.huanglin.pro.domain">

   <class name="Picture" table="tbl_picture">
       <id name="pId" column="id" type="java.lang.Integer" length="6">
         <generator class="assigned"></generator>
       </id>
       <property name="pUrl"   column="p_url"     type="java.lang.String" length="50" not-null="false"></property>   
       <property name="pTitle" column="p_title"      type="java.lang.String" length="100" not-null="false"></property>   
       <property name="pInfo"  column="p_info"    type="java.lang.String" length="100" not-null="false"></property>  
   </class>

</hibernate-mapping>