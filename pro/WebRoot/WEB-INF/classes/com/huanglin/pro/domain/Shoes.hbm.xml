<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping package="com.huanglin.pro.domain">

   <class name="Shoes" table="shoes">
   
       <id name="shoesId" column="id" type="java.lang.Integer">
         <generator class="assigned"></generator>
       </id>
       <property name="shoesName"    column="s_name"   type="java.lang.String" length="100" not-null="true"></property>   
       <property name="ShoesUrl"     column="s_pngurl" type="java.lang.String" length="100" not-null="true"></property>   
       <property name="shoesPrise"   column="s_price"  type="java.lang.Double" length="4"   not-null="true"></property>   
       <property name="shoesSize"    column="s_size"   type="java.lang.Integer" length="4"   not-null="true"></property>   
       <property name="shoesColor"   column="s_color"  type="java.lang.String" length="10"  not-null="false"></property>   
       <property name="shoesSex"     column="s_sex"    type="char"             length="1"   not-null="false"></property>   
       <property name="shoesBrand"   column="s_brand"  type="java.lang.String" length="20"  not-null="false"></property>   
       <property name="shoesStyle"   column="s_style"  type="java.lang.String" length="10"  not-null="false"></property>   
       <property name="shoesOrign"   column="s_origin" type="java.lang.String" length="10"  not-null="false"></property>   
       <property name="nub"          column="shoes_num" type="java.lang.Integer" length="11"  not-null="false"></property>
          
   </class>
 
</hibernate-mapping>